<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.masinsa.mapper.MaskMapper">

  <select id="getAllImages" resultType="string">
    SELECT image_url FROM image WHERE mask_id = #{maskId};
  </select>
  
  <select id="getAllImagesOb" resultType="com.spring.masinsa.entity.Image">
    SELECT * FROM image WHERE mask_id = #{maskId};
  </select>

</mapper>